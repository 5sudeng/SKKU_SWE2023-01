import java.awt.Point;
import java.awt.Color;

public class DrawData {

	public Point ptDot; // get set 메소드 생성 하지 않고, 구조체 형태로 사용
	public Color color;
	public int dotSize;
	
	public DrawData() {
		ptDot = new Point();
		dotSize = 10;
	}
	
	public DrawData(Point pt, int size) {
		ptDot = pt;
		dotSize = size;
	}
} // DrawData class
